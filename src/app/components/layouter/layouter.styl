  }

  .pagina {
    width: 1122px;
    min-height: 794px;
    margin: 30px auto;
    border: 1px #D3D3D3 solid;
    background: white;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
    background-size: cover;
  }

  .moldura {
    width: 100%;
    height: 100%;
    padding: 60px;
    background: url(../assets/images/moldura.png);
    background-size: 100%;
  }

  .ctn {
    height: 674px;
  }

  .ctn > img {
    width: 200px;
  }

  .draggable {
    position: relative;
    border: 1px solid #fff;
    width: 420px;
  }

  .draggableContent {
    border: 1px solid transparent;

    &:not(.img-responsive) {
      height: 100%;
    }
  }

  .draggable:hover .draggableContent,
  .draggable.hover .draggableContent{
    border: 1px solid lightgrey;
    cursor: drag;
    display: block;
  }

  .draggable.is-dragging { opacity: 0.7; }

  .draggable:hover .draggableHandle,
  .draggable:hover .draggableToolbar,
  .draggable.hover .draggableHandle,
  .draggable.hover .draggableToolbar{
    display: block;
  }

  .draggableHandle {
    display: none;
    cursor: move;
    width: 24px;
    height: 24px;
    background: lightgrey;
    position: absolute;
    top: -25px;
    text-align: center;

    > i {
      vertical-align: middle;
    }
  }


  .draggableToolbar {
    display: none;
    position: absolute;
    top: -29px;
    right: 0;
    cursor: pointer;
    border-radius: 6px 6px 0px 0px;
    background: lightgrey;
    padding: 2px 6px;
    z-index: 99;

    > a {
      display: inline-block;
      text-align: center;
      color: black;
      vertical-align: middle;
    }

    select {
      display: inline-block;
      vertical-align: middle;
    }
  }
}

/* Toolbar */

.toolbar {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: -webkit-box;
  display: flex;
  position: relative;
  height: 64px;
  padding: 12.5px 8px;
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  color: #FFF;
  background-color: #009688;
  box-shadow: 0 1px 4px 0 rgba(0, 0, 0, 0.37);
}

.toolbar .toolbar-title {
  display: -webkit-inline-flex;
  display: -ms-inline-flexbox;
  display: -webkit-inline-box;
  display: inline-flex;
  vertical-align: middle;
  font-size: 21px;
  -webkit-flex-grow: 1;
  -ms-flex-positive: 1;
  -webkit-box-flex: 1;
  flex-grow: 1;
  margin: 0 8px;
  padding: 7px 0;
}

.toolbar .icon-button {
  display: inline-block;
  border-radius: 2px;
  padding: 7px;
  margin: 0 8px;
  vertical-align: middle;
  cursor: pointer;
  fill: #FFF;
  transition: box-shadow 0.15s cubic-bezier(0.4, 0, 0.2, 1);
  width: 39px;
  text-align: center;
  font-size: 20px;

  .fa {
    vertical-align: inherit;
  }
}

.toolbar .icon-button:hover {
  box-shadow: 0 1px 0 0 rgba(0, 0, 0, 0.12), 0 0 0 1px rgba(0, 0, 0, 0.1);
}

.toolbar .icon-button svg {
  display: inline-block;
  vertical-align: middle;
  position: relative;
  height: 24px;
  width: 24px;
}

.toolbar [content]:not(.tooltip) {
  position: absolute;
  height: calc(100% - 64px);
  width: calc(100% - 265px);
  overflow: auto;
}

  .toolbar .popover-title {
    color: #333;
  }
/* End Toolbar */
